<p id="notice"><%= notice %></p>

<h1>Indcategories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @indcategories.each do |indcategory| %>
      <tr>
        <td><%= indcategory.name %></td>
        <td><%= indcategory.description %></td>
        <td><%= link_to 'Show', indcategory %></td>
        <td><%= link_to 'Edit', edit_indcategory_path(indcategory) %></td>
        <td><%= link_to 'Destroy', indcategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Indcategory', new_indcategory_path %>
