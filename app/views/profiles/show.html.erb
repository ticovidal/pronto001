<%- model_class = Profile -%>
<div class="page-header">
  <h1><%=t '.title', default: model_class.model_name.human.titleize %></h1>
</div>

<h2 style="border: 1px solid grey; -webkit-border-radius:50%; -mos-border-radius:50%; -ms-border-radius:50%; border-radius:50%; font-weight:bold">
 <%= @profile.name.split.map(&:first).join %>
</h2>
<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:id) %></dt>
      <dd><pre class="prettyprint"><%= @profile.id %></pre></dd>
      <h1><%= current_user.profile.id %></h1>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @profile.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:email) %></dt>
      <dd><pre class="prettyprint"><%= @profile.email %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cpf) %></dt>
      <dd><pre class="prettyprint"><%= @profile.cpf %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone) %></dt>
      <dd><pre class="prettyprint"><%= @profile.phone %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cellphone) %></dt>
      <dd><pre class="prettyprint"><%= @profile.cellphone %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:street) %></dt>
      <dd><pre class="prettyprint"><%= @profile.street %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:number) %></dt>
      <dd><pre class="prettyprint"><%= @profile.number %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:compl) %></dt>
      <dd><pre class="prettyprint"><%= @profile.compl %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:neighbor) %></dt>
      <dd><pre class="prettyprint"><%= @profile.neighbor %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:city) %></dt>
      <dd><pre class="prettyprint"><%= @profile.city %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:country) %></dt>
      <dd><pre class="prettyprint"><%= @profile.country %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:company) %></dt>
      <dd><pre class="prettyprint"><%= @profile.company %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:social) %></dt>
      <dd><pre class="prettyprint"><%= @profile.social %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cnpj) %></dt>
      <dd><pre class="prettyprint"><%= @profile.cnpj %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:user_id) %></dt>
      <dd><pre class="prettyprint"><%= @profile.user_id %></pre></dd>
      <h1><%= current_user.id %></h1>
      <dt class="label label-default"><%= model_class.human_attribute_name(:profileable_type) %></dt>
      <dd><pre class="prettyprint"><%= @profile.profileable_type %></pre></dd>
      <h1><%= current_user.profile.profileable_type %></h1>
      <dt class="label label-default"><%= model_class.human_attribute_name(:profileable_id) %></dt>
      <dd><pre class="prettyprint"><%= @profile.profileable_id %></pre></dd>
      <h1><%= current_user.profile.profileable_id %></h1>
      <dt class="label label-default"><%= model_class.human_attribute_name(:indcategory) %></dt>
      <dd><pre class="prettyprint"><% if @profile.profileable_type == "industry" %><%= @type_options_for_select.name if @type_options_for_select %><% elsif @profile.profileable_type == "provider" %><% @type_options_for_select.each do |ty| %><%= ty.name %><br/><% end %><% end %></pre></dd>

  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', default: t("helpers.links.back")),
              profiles_path, class: 'btn btn-default'  %>
  <%= link_to t('.edit', default: t("helpers.links.edit")),
              edit_profile_path(@profile), class: 'btn btn-default' %>
  <%= link_to t('.destroy', default: t("helpers.links.destroy")),
              profile_path(@profile),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) },
              class: 'btn btn-danger' %>
</div>