<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Cpf</th>
      <th>Phone</th>
      <th>Cellphone</th>
      <th>Street</th>
      <th>Number</th>
      <th>Compl</th>
      <th>Neighbor</th>
      <th>City</th>
      <th>Country</th>
      <th>Company</th>
      <th>Social</th>
      <th>Cnpj</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.email %></td>
        <td><%= profile.cpf %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.cellphone %></td>
        <td><%= profile.street %></td>
        <td><%= profile.number %></td>
        <td><%= profile.compl %></td>
        <td><%= profile.neighbor %></td>
        <td><%= profile.city %></td>
        <td><%= profile.country %></td>
        <td><%= profile.company %></td>
        <td><%= profile.social %></td>
        <td><%= profile.cnpj %></td>
        <td><%= profile.user %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
